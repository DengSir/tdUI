<Ui xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
    <Frame name="tdUICollectFrameTemplate" hidden="true" enableMouse="true" virtual="true">
        <Backdrop bgFile="Interface\BUTTONS\WHITE8x8" edgeFile="Interface\Tooltips\UI-Tooltip-Border" tile="true">
            <EdgeSize val="16" />
            <TileSize val="16" />
            <BackgroundInsets left="4" right="4" top="4" bottom="4" />
        </Backdrop>
        <Layers>
            <Layer level="OVERLAY" textureSubLevel="1">
                <FontString parentKey="Title" inherits="GameFontNormalOutline">
                    <Anchors>
                        <Anchor point="BOTTOMLEFT" relativePoint="TOPLEFT" x="15" y="-6" />
                    </Anchors>
                </FontString>
            </Layer>
            <Layer level="OVERLAY">
                <Texture file="Interface/Scenarios/ScenarioParts">
                    <Size x="34" y="34" />
                    <Anchors>
                        <Anchor point="TOPLEFT" x="1" y="-1" />
                    </Anchors>
                    <TexCoords left="0.48046875" right="0.546875" top="0.345703125" bottom="0.412109375" />
                </Texture>
                <Texture file="Interface/Scenarios/ScenarioParts">
                    <Size x="34" y="34" />
                    <Anchors>
                        <Anchor point="TOPRIGHT" x="-1" y="-1" />
                    </Anchors>
                    <TexCoords left="0.546875" right="0.48046875" top="0.345703125" bottom="0.412109375" />
                </Texture>
                <Texture file="Interface/Scenarios/ScenarioParts">
                    <Size x="34" y="34" />
                    <Anchors>
                        <Anchor point="BOTTOMLEFT" x="1" y="1" />
                    </Anchors>
                    <TexCoords left="0.48046875" right="0.546875" top="0.412109375" bottom="0.345703125" />
                </Texture>
                <Texture file="Interface/Scenarios/ScenarioParts">
                    <Size x="34" y="34" />
                    <Anchors>
                        <Anchor point="BOTTOMRIGHT" x="-1" y="1" />
                    </Anchors>
                    <TexCoords left="0.546875" right="0.48046875" top="0.412109375" bottom="0.345703125" />
                </Texture>
            </Layer>
        </Layers>
        <Frames>
            <Button parentKey="Edit" hidden="true">
                <Size x="16" y="16" />
                <Anchors>
                    <Anchor point="LEFT" relativeKey="$parent.Title" relativePoint="RIGHT" x="5" y="-3" />
                </Anchors>
                <NormalTexture file="Interface\Buttons\CancelButton-Up">
                    <Size x="32" y="32" />
                    <Anchors>
                        <Anchor point="CENTER" />
                    </Anchors>
                </NormalTexture>
                <PushedTexture file="Interface\Buttons\CancelButton-Down">
                    <Size x="32" y="32" />
                    <Anchors>
                        <Anchor point="CENTER" />
                    </Anchors>
                </PushedTexture>
                <HighlightTexture file="Interface\Buttons\CancelButton-Highlight" alphaMode="ADD">
                    <Size x="32" y="32" />
                    <Anchors>
                        <Anchor point="CENTER" />
                    </Anchors>
                </HighlightTexture>
                <Scripts>
                    <OnClick>
                        self:Hide()
                    </OnClick>
                    <OnEnter>
                        GameTooltip:SetOwner(self, 'ANCHOR_LEFT')
                        GameTooltip:SetText(self.tooltip)
                    </OnEnter>
                    <OnLeave function="GameTooltip_Hide" />
                </Scripts>
            </Button>
            <Button parentKey="Option">
                <Size x="16" y="16" />
                <Anchors>
                    <Anchor point="TOPRIGHT" x="-25" y="8" />
                </Anchors>
                <NormalTexture file="Interface\Scenarios\ScenarioIcon-Interact" />
                <HighlightTexture file="Interface\Scenarios\ScenarioIcon-Interact" alphaMode="ADD" />
            </Button>
            <Frame parentKey="AutoHide">
                <Anchors>
                    <Anchor point="BOTTOMLEFT" />
                </Anchors>
                <Scripts>
                    <OnUpdate>
                        if self:IsMouseOver() or self:GetParent().Edit:IsShown() then
                            self.timer = nil
                        else
                            self.timer = (self.timer or 2) - elapsed
                            if self.timer > 0 then
                            else
                                self:GetParent():Hide()
                            end
                        end
                    </OnUpdate>
                    <OnShow>
                        self.timer = nil
                    </OnShow>
                </Scripts>
            </Frame>
        </Frames>
        <Scripts>
            <OnLoad>
                self:SetBackdropBorderColor(1, 0.82, 0)
                self:SetBackdropColor(0.1, 0.082, 0, 0.8)
            </OnLoad>
            <OnHide>
                self:Hide()
            </OnHide>
        </Scripts>
    </Frame>
    <Button name="tdUICollectEditButtonTemplate" hidden="true" setAllPoints="true" virtual="true">
        <NormalTexture file="Interface\Minimap\UI-Minimap-ZoomButton-Highlight">
            <Size x="20" y="20" />
            <Anchors>
                <Anchor point="CENTER" x="-8" y="-8" />
            </Anchors>
        </NormalTexture>
        <PushedTexture file="Interface\Minimap\UI-Minimap-ZoomButton-Highlight">
            <Size x="20" y="20" />
            <Anchors>
                <Anchor point="CENTER" x="-8" y="-8" />
            </Anchors>
        </PushedTexture>
        <HighlightTexture file="Interface\Minimap\UI-Minimap-ZoomButton-Highlight" alphaMode="ADD">
            <Size x="20" y="20" />
            <Anchors>
                <Anchor point="CENTER" x="-8" y="-8" />
            </Anchors>
        </HighlightTexture>
        <Scripts>
            <OnLoad>
                self:SetFrameLevel(self:GetParent():GetFrameLevel() + 20)
            </OnLoad>
        </Scripts>
    </Button>
    <Frame name="tdUIMicroButtonAndBagsBarTemplate" virtual="true">
        <Size x="1" y="87" />
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="BgLeft" file="Interface\MainMenuBar\MainMenuBar">
                    <Size x="30" y="43" />
                    <Anchors>
                        <Anchor point="BOTTOMLEFT" />
                    </Anchors>
                </Texture>
                <Texture parentKey="BgRight" file="Interface\MainMenuBar\MainMenuBar">
                    <Size x="30" y="43" />
                    <Anchors>
                        <Anchor point="BOTTOMRIGHT" />
                    </Anchors>
                </Texture>
                <Texture parentKey="BgMiddle" file="Interface\MainMenuBar\MainMenuBar">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeKey="$parent.BgLeft" relativePoint="TOPRIGHT" />
                        <Anchor point="BOTTOMRIGHT" relativeKey="$parent.BgRight" relativePoint="BOTTOMLEFT" />
                    </Anchors>
                </Texture>
                <Texture parentKey="BgBag" file="Interface\MainMenuBar\MainMenuBar">
                    <Size x="184" y="45" />
                    <Anchors>
                        <Anchor point="TOPRIGHT" />
                    </Anchors>
                </Texture>
                <Texture parentKey="BgKeyring" file="Interface\MainMenuBar\MainMenuBar">
                    <Size x="24" y="45" />
                    <Anchors>
                        <Anchor point="TOPRIGHT" />
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
    </Frame>
    <Button name="tdUIAuctionBrowseItemTemplate" virtual="true">
        <Size x="1" y="18" />
        <Frames>
            <Button parentKey="Enter">
                <Size x="18" y="18" />
                <Anchors>
                    <Anchor point="LEFT"/>
                </Anchors>
                <Scripts>
                    <OnEnter>
                        local parent = self:GetParent()
                        parent:LockHighlight()
                        AuctionFrameItem_OnEnter(self, 'list', parent.id)
                    </OnEnter>
                    <OnLeave>
                        local parent = self:GetParent()
                        parent:UnlockHighlight()
                        GameTooltip_Hide()
                        ResetCursor()
                    </OnLeave>
                    <OnUpdate>
                        if ( GameTooltip:IsOwned(self) ) then
                            AuctionFrameItem_OnEnter(self, 'list', self:GetParent():GetID())
                        end
                    </OnUpdate>
                </Scripts>
            </Button>
        </Frames>
        <Layers>
            <Layer level="ARTWORK">
                <Texture parentKey="Icon">
                    <Size x="18" y="18" />
                    <Anchors>
                        <Anchor point="LEFT"/>
                    </Anchors>
                </Texture>
                <FontString parentKey="Name" inherits="GameFontHighlightSmallLeft" wordWrap="false">
                    <Size x="142"/>
                    <Anchors>
                        <Anchor point="LEFT" relativePoint="LEFT" x="20" y="0" />
                    </Anchors>
                </FontString>
                <FontString parentKey="Level" inherits="GameFontHighlightSmall">
                    <Anchors>
                        <Anchor point="CENTER" relativePoint="LEFT" x="175" y="0" />
                    </Anchors>
                </FontString>
                <FontString parentKey="Time" inherits="GameFontHighlightSmall">
                    <Anchors>
                        <Anchor point="CENTER" relativePoint="LEFT" x="225" y="0" />
                    </Anchors>
                </FontString>
                <FontString parentKey="Seller" inherits="GameFontHighlightSmall" wordWrap="false">
                    <Size x="73"/>
                    <Anchors>
                        <Anchor point="CENTER" relativePoint="LEFT" x="295" y="0" />
                    </Anchors>
                </FontString>

                <FontString parentKey="UnitPrice" inherits="GameFontHighlightSmallRight">
                    <Anchors>
                        <Anchor point="RIGHT" x="-3" y="0" />
                    </Anchors>
                </FontString>
                <FontString parentKey="Buyout" inherits="GameFontHighlightSmallRight">
                    <Anchors>
                        <Anchor point="RIGHT" x="-103" y="0" />
                    </Anchors>
                </FontString>
                <FontString parentKey="Bid" inherits="GameFontHighlightSmallRight">
                    <Anchors>
                        <Anchor point="RIGHT" x="-203" y="0" />
                    </Anchors>
                </FontString>
            </Layer>
            <Layer level="BACKGROUND">
                <Texture parentKey="Selected" hidden="true" setAllPoints="true">
                    <Color r="1" g="0.81" b="0" a="0.3" />
                </Texture>
                <Texture parentKey="Highlight" setAllPoints="true">
                    <Color r="1" g="0.81" b="0" a="0.3" />
                </Texture>
            </Layer>
            <Layer level="BACKGROUND" textureSubLevel="-1">
                <Texture parentKey="Bg" hidden="true" setAllPoints="true">
                    <Color r="1" g="1" b="1" a="0.1" />
                </Texture>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad>
                self:SetHighlightTexture(self.Highlight)
            </OnLoad>
        </Scripts>
    </Button>
    <ScrollFrame name="tdUIAuctionBrowseScrollFrameTemplate" inherits="HybridScrollFrameTemplate" virtual="true">
        <Size x="612" y="304" />
        <Anchors>
            <Anchor point="TOPLEFT" x="190" y="-104" />
        </Anchors>
        <Frames>
            <Slider parentKey="scrollBar" inherits="HybridScrollBarTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="TOPRIGHT" x="0" y="-15"/>
                    <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMRIGHT" x="0" y="12"/>
                </Anchors>
            </Slider>
        </Frames>
    </ScrollFrame>
    <Button name="tdUIBonusItemTemplate" virtual="true">
        <Size x="30" y="30" />
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="Icon" />
            </Layer>
            <Layer level="OVERLAY" textureSubLevel="1">
                <Texture parentKey="Class" file="Interface\TargetingFrame\UI-Classes-Circles">
                    <Size x="16" y="16"/>
                    <Anchors>
                        <Anchor point="BOTTOMRIGHT" x="-2" y="-7" />
                    </Anchors>
                </Texture>
            </Layer>
            <Layer level="OVERLAY" textureSubLevel="2">
                <Texture parentKey="ClassBorder" file="Interface\Common\RingBorder">
                    <Size x="20" y="20" />
                    <Anchors>
                        <Anchor point="CENTER" relativeKey="$parent.Class" />
                    </Anchors>
                </Texture>
            </Layer>
            <Layer level="OVERLAY" textureSubLevel="-2">
                <Texture parentKey="Border" file="Interface\Buttons\UI-ActionButton-Border" alphaMode="ADD">
                    <Size x="58" y="58" />
                    <Anchors>
                        <Anchor point="CENTER" />
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
        <NormalTexture file="Interface\Buttons\UI-Quickslot2">
            <Size x="53" y="53" />
            <Anchors>
                <Anchor point="CENTER" x="0" y="-1" />
            </Anchors>
        </NormalTexture>
        <HighlightTexture file="Interface\Buttons\ButtonHilight-Square" alphaMode="ADD"/>
        <PushedTexture file="Interface\Buttons\UI-Quickslot-Depress"/>
        <Scripts>
            <OnLoad>
                self.UpdateTooltip = self:GetScript('OnEnter')
            </OnLoad>
            <OnClick>
                HandleModifiedItemClick(select(2, GetItemInfo(self.itemId)))
            </OnClick>
            <OnEnter>
                GameTooltip:SetOwner(self, 'ANCHOR_TOP')
                GameTooltip:SetItemByID(self.itemId)
                CursorUpdate(self)
            </OnEnter>
            <OnLeave function="GameTooltip_Hide"/>
        </Scripts>
    </Button>
    <Frame name="tdUIBonusFrame" enableMouse="true" movable="true" clampedToScreen="true" hidden="true" parent="UIParent">
        <Size x="1" y="56"/>
        <Anchors>
            <Anchor point="BOTTOM" relativePoint="BOTTOM" x="0" y="200" />
        </Anchors>
        <Backdrop bgFile="Interface\Tooltips\UI-Tooltip-Background" edgeFile="Interface\Tooltips\UI-Tooltip-Border" tile="true">
            <EdgeSize val="16" />
            <TileSize val="16" />
            <BackgroundInsets left="4" right="4" top="4" bottom="4" />
        </Backdrop>
        <Frames>
            <Button parentKey="Token" inherits="tdUIBonusItemTemplate" registerForClicks="LeftButtonUp,RightButtonUp">
                <Anchors>
                    <Anchor point="TOPLEFT" x="12" y="-30" />
                </Anchors>
            </Button>
            <Button parentKey="Close" inherits="UIPanelCloseButton">
                <Size x="16" y="16"/>
                <Anchors>
                    <Anchor point="TOPRIGHT" x="-10" y="-10" />
                </Anchors>
                <NormalTexture file="Interface\Buttons\UI-Panel-MinimizeButton-Up">
                    <TexCoords left="0.15625" right="0.84375" top="0.15625" bottom="0.84375" />
                </NormalTexture>
                <PushedTexture file="Interface\Buttons\UI-Panel-MinimizeButton-Down">
                    <TexCoords left="0.15625" right="0.84375" top="0.15625" bottom="0.84375" />
                </PushedTexture>
                <HighlightTexture file="Interface\Buttons\UI-Panel-MinimizeButton-Highlight" alphaMode="ADD">
                    <TexCoords left="0.15625" right="0.84375" top="0.15625" bottom="0.84375" />
                </HighlightTexture>
            </Button>
            <Button parentKey="ChatButton">
                <Size x="16" y="16"/>
                <Anchors>
                    <Anchor point="RIGHT" relativeKey="$parent.Close" relativePoint="LEFT" x="-4" y="0" />
                </Anchors>
                <Scripts>
                    <OnClick>
                        self:GetParent():OpenChatMenu(self)
                    </OnClick>
                </Scripts>
                <NormalTexture file="Interface\ChatFrame\UI-ChatWhisperIcon"/>
                <HighlightTexture file="Interface\ChatFrame\UI-ChatWhisperIcon" alphaMode="ADD"/>
            </Button>
        </Frames>
        <Layers>
            <Layer level="ARTWORK">
                <Texture file="Interface\Transmogrify\transmog-tooltip-arrow">
                    <Size x="12" y="12"/>
                    <Anchors>
                        <Anchor point="LEFT" relativeKey="$parent.Token" relativePoint="RIGHT" x="4" y="0" />
                    </Anchors>
                </Texture>
                <FontString parentKey="Name" justifyH="LEFT" wordWrap="false" inherits="GameTooltipHeaderText">
                    <Size y="16"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" x="10" y="-10" />
                        <Anchor point="RIGHT" relativeKey="$parent.ChatButton" relativePoint="LEFT" x="-4" y="0" />
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad>
                self:SetBackdropColor(0.09, 0.09, 0.19)
                self:SetUserPlaced(true)
                self:RegisterForDrag('LeftButton')
            </OnLoad>
            <OnHide>
                self:Hide()
            </OnHide>
            <OnDragStart>
                self:StartMoving()
            </OnDragStart>
            <OnDragStop>
                self:StopMovingOrSizing()
            </OnDragStop>
        </Scripts>
    </Frame>
</Ui>
